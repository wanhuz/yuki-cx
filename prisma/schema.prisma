generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_PRISMA")
}

model Settings {
  id    Int     @id @default(autoincrement())
  key   String  @unique
  value String?
}

model ServiceMapping {
  id      Int     @id @default(autoincrement())
  anidbid Int?
  tvdbid  Int?
  tmdbid  Int?
  imdbid  Int?
  name    String?
  studio  String?
}
